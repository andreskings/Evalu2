    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/whatsapp_image_2023_10_20_at_1_17_14_am"
        android:label="@string/app_name"
        android:roundIcon="@drawable/whatsapp_image_2023_10_20_at_1_17_14_am"
        android:supportsRtl="true"
        android:theme="@style/Theme.Evalu2"
        tools:targetApi="31">

